<web-app>

	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>datainput</servlet-name>
		<jsp-file>/data_input.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>datainput</servlet-name>
		<url-pattern>/data_input</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>main</servlet-name>
		<jsp-file>/main.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>main</servlet-name>
		<url-pattern>/main</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>result</servlet-name>
		<jsp-file>/result.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>result</servlet-name>
		<url-pattern>/result</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>account</servlet-name>
		<jsp-file>/account.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>account</servlet-name>
		<url-pattern>/account</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>aboutus</servlet-name>
		<jsp-file>/about_us.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>aboutus</servlet-name>
		<url-pattern>/aboutus</url-pattern>
	</servlet-mapping>


	<filter>
		<filter-name>LoginCheckFilter</filter-name>
		<filter-class>LoginCheckFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>LoginCheckFilter</filter-name>
		<url-pattern>/data_input</url-pattern>
		<dispatcher>REQUEST</dispatcher>
		<dispatcher>FORWARD</dispatcher>
	</filter-mapping>


	<servlet>
		<servlet-name>authenticate</servlet-name>
		<servlet-class>AuthenticateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>authenticate</servlet-name>
		<url-pattern>/authenticate</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>PasswordChangeServlet</servlet-name>
		<servlet-class>PasswordChangeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PasswordChangeServlet</servlet-name>
		<url-pattern>/passwordchangeservlet</url-pattern>
	</servlet-mapping>


  <servlet>
    <servlet-name>DataInputServlet</servlet-name>
    <servlet-class>JDBC.DataInputServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DataInputServlet</servlet-name>
    <url-pattern>/datainputservlet</url-pattern>
  </servlet-mapping>

  
	<servlet>
		<servlet-name>ShowResultServlet</servlet-name>
		<servlet-class>ShowResultServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShowResultServlet</servlet-name>
		<url-pattern>/showresultservlet</url-pattern>
	</servlet-mapping>

</web-app>